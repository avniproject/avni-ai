{
  "type": "object",
  "properties": {
    "response": {
      "type": "string"
    },
    "config": {
      "type": "object",
      "properties": {
        "addressLevelTypes": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "name": {"type": "string"},
              "uuid": {"type": "string"},
              "voided": {"type": "boolean"},
              "level": {"type": "number"},
              "parentId": {"type": ["integer", "null"]}
            },
            "required": ["name", "level"]
          }
        },
        "locations": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "name": {"type": "string"},
              "uuid": {"type": "string"},
              "voided": {"type": "boolean"},
              "level": {"type": "number"},
              "type": {"type": "string"},
              "addressLevelTypeUUID": {"type": "string"},
              "organisationUUID": {"type": "string"},
              "legacyId": {"type": ["string", "null"]},
              "gpsCoordinates": {"type": ["object", "null"]},
              "locationProperties": {"type": "object"}
            },
            "required": ["name", "level", "type", "addressLevelTypeUUID", "organisationUUID"]
          }
        },
        "subjectTypes": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "name": {"type": "string"},
              "type": {"type": "string"},
              "group": {"type": "boolean"},
              "household": {"type": "boolean"},
              "active": {"type": "boolean"},
              "subjectSummaryRule": {"type": ["string", "null"]},
              "programEligibilityCheckRule": {"type": ["string", "null"]},
              "memberAdditionEligibilityCheckRule": {"type": ["string", "null"]},
              "allowEmptyLocation": {"type": "boolean"},
              "allowMiddleName": {"type": "boolean"},
              "lastNameOptional": {"type": "boolean"},
              "allowProfilePicture": {"type": "boolean"},
              "uniqueName": {"type": "boolean"},
              "directlyAssignable": {"type": "boolean"},
              "shouldSyncByLocation": {"type": "boolean"}
            },
            "required": ["name", "type"]
          }
        },
        "programs": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "name": {"type": "string"},
              "colour": {"type": "string"},
              "voided": {"type": "boolean"},
              "active": {"type": "boolean"},
              "enrolmentEligibilityCheckRule": {"type": ["string", "null"]},
              "enrolmentSummaryRule": {"type": ["string", "null"]},
              "manualEligibilityCheckRequired": {"type": "boolean"},
              "showGrowthChart": {"type": "boolean"},
              "allowMultipleEnrolments": {"type": "boolean"}
            },
            "required": ["name", "colour"]
          }
        },
        "encounterTypes": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "name": {"type": "string"},
              "entityEligibilityCheckRule": {"type": ["string", "null"]},
              "active": {"type": "boolean"},
              "isImmutable": {"type": "boolean"},
              "voided": {"type": "boolean"}
            },
            "required": ["name"]
          }
        }
      }
    },
    "context": {
      "type": "object",
      "properties": {
        "action": {
          "type": "string",
          "enum": ["verify_entities"]
        },
        "entities": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "type": {
                "type": "string",
                "enum": ["addressLevelTypes", "locations", "subjectTypes", "programs", "encounterTypes"]
              },
              "name": {
                "type": "string"
              }
            },
            "required": ["type", "name"]
          }
        }
      }
    },
    "loop": {
      "type": "boolean"
    }
  },
  "required": ["response", "config", "context", "loop"],
  "additionalProperties": false
}